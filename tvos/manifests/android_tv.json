{
  "api": {
    "require_startup_execution": false,
    "class_name": "com.applicaster.reactnative.plugins.APReactNativeAdapter",
    "react_packages": [
      "com.tva.quickbrickplayerplugin.TVAQuickBrickPlayerPluginReactPackage"
    ],
    "proguard_rules": "-keep class com.tva.quickbrickplayerplugin.** { *; }\n-keep class com.bitmovin.player.** { *; }\n"
  },
  "dependency_repository_url": [
    {
      "url": "http://bitmovin.bintray.com/maven"
    }
  ],
  "dependency_name": "@applicaster/quick-brick-television-academy-bitmovin-player",
  "dependency_version": "0.4.9",
  "platform": "android",
  "author_name": "Vlad Sumtsov",
  "author_email": "vlad.sumtsov@corewillsoft.com",
  "manifest_version": "0.4.9",
  "name": "Television Academy Player TV",
  "description": "Television Academy Bitmovin Player plugin for Android TV on Quick Brick",
  "type": "player",
  "screen": true,
  "identifier": "TelevisionAcademyPlayerPluginTV",
  "whitelisted_account_ids": [
    "5dcaba3ee648eb000eaf5c0a"
  ],
  "ui_builder_support": true,
  "min_zapp_sdk": "0.1.0",
  "deprecated_since_zapp_sdk": "",
  "unsupported_since_zapp_sdk": "",
  "react_native": true,
  "react_bundle_url": "",
  "extra_dependencies": [],
  "npm_dependencies": [
    "@applicaster/quick-brick-television-academy-bitmovin-player@0.4.9"
  ],
  "project_dependencies": [
    {
      "tva-quick-brick-player": "node_modules/@applicaster/quick-brick-television-academy-bitmovin-player/android"
    }
  ],
  "targets": [
    "tv"
  ],
  "custom_configuration_fields": [
    {
      "key": "testVideoSrc",
      "type": "text",
      "tooltip_text": "Url which will replace url from dsp for test purposes. Example: https://bitdash-a.akamaihd.net/content/sintel/sintel.mpd",
      "default": ""
    },
    {
      "key": "baseSkylarkUrl",
      "type": "text",
      "tooltip_text": "Url for skylark to put video metadata. Example: https://publicapi.feature.atas.ostm.io/api/v1",
      "default": ""
    }
  ],
  "hooks": {
    "fields": [
      {
        "group": true,
        "label": "Before Load",
        "folded": true,
        "fields": [
          {
            "key": "preload_plugins",
            "type": "preload_plugins_selector",
            "label": "Select Plugins"
          }
        ]
      }
    ]
  }
}
